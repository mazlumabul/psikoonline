package scorpion.psikonline.model;

import java.util.UUID;

import javax.persistence.Entity;
import javax.persistence.Id;

import lombok.Data;

@Data
@Entity
public class User {
	
	@Id
	private UUID id;
	private String tckn;
	private String name;
	private String userName;
	private String phone;
	private String email;
	private String password;
	private String role;
}
